GridLayout:


<GridLayout>:
   spacing:"30dp"
   rows:3
   BoxLayout:
      size_hint:"100dp",None
      height:"40dp"
      Button:
         text:"setting"
         size_hint:None,None
         width:"60dp"
         height:"40dp"
         pos_hint:{"top":1}

      Label:
         text:"mathcathalon"


      Button:
         text:"pause"
         size_hint:None,None
         width:"60dp"
         height:"40dp"
         pos_hint:{"right":1}
         pos_hint:{"top":1}
   BoxLayout:
      orientation: "vertical"
      #:set x dp(400)
      #:set y dp(150)
      Label:
         canvas:
            Rectangle:
               pos: self.center_x-x/2,self.center_y-y/2
               size:x,y

      Button:
         text:"f"
         size_hint:None,None
         size: "300dp","100dp"
         pos_hint:{"center_x":.5}


   AnchorLayout:
      size_hint:.2,.2
      BoxLayout:
         orientation:"vertical"
         Button:
            text: "Submit"
            size_hint:None,None
            size:"200dp","40dp"
            pos_hint: {"center_x": .5}

         Button:
            text:"i"
            size_hint: None,None
            pos_hint:{"x":.0}
            size:"40dp","40dp"
<AnchorLayoutPause>:
   anchor_x:"right"
   anchor_y:"top"
   BoxLayout:
      orientation:"vertical"
      size_hint:.1,.3
      Button:
         text:"play"
      ToggleButton:
         text:"music"
      Button:
         text:"quit"
<AnchorLayoutQuit>:
   anchor_y:"center"

   BoxLayout:
      size_hint: None,None
      size: "200dp","50dp"
      Button:
         text:"Yes"
      Button:
         text:"No"

<AnchorLayoutSettings>:
   anchor_x:"left"
   anchor_y:"top"
   BoxLayout:
      orientation:"vertical"
      size_hint:.1,.3
      Button:
         text:"Level"
      Button:
         text:"Share"
      Button:
         text:"Rate us"
      Button:
         text:"about us"



